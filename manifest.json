{
  "name": "pico",
  "version": "1.0",
  "description": "Get you color codes here",
  "permissions": ["storage"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/get_started16.png",
      "32": "img/get_started32.png",
      "48": "img/get_started48.png",
      "128": "img/get_started128.png"
    }
  },
  "manifest_version": 2
}
